{
  "buildCommand": "vite build --outDir dist/public",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/questions/:category",
      "destination": "/api/index.js"
    }
  ],
  "routes": [
    {
      "src": "/api/questions/(.+)",
      "dest": "/api/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
